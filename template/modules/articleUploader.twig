{% block articlesUpModule %}
    <form method="POST">
        <div>
            <label for="title">Title: </label>
            <input type="text" id="title" name="userWriteTitle"/>
        </div>
        <div>
            <label for="userWriteNews">Article: </label>
            <input type="text" id="userWriteNews" name="userWriteTitle"/>
            <textarea rows="7" cols="50" name="userWriteNews"></textarea>
        </div>
        <div>
            <input type="submit" name="userUploadNews" value="SendIn"/>
            <input type="reset" name="resetUploadNews" value="Cancel"/>
        </div>
    </form>
{% endblock %}